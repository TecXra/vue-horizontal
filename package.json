{
  "name": "vue-horizontal",
  "version": "0.0.0",
  "description": "An ultra simple pure vue horizontal layout for modern responsive web with zero dependencies.",
  "author": "Fuxing Loh",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuxingloh/vue-horizontal.git"
  },
  "bugs": "https://github.com/fuxingloh/vue-horizontal/issues",
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "nuxt",
    "horizontal",
    "list view",
    "scroll view"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "size-limit": [
    {
      "path": "./dist/vue-horizontal.umd.js"
    },
    {
      "path": "./dist/vue-horizontal.es.js"
    }
  ],
  "main": "./dist/vue-horizontal.umd.js",
  "module": "./dist/vue-horizontal.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-horizontal.es.js",
      "require": "./dist/vue-horizontal.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "size": "size-limit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "devDependencies": {
    "vue": "^3.0.5",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.4.1",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.12",
    "vue-router": "^4.0.3"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "browserslist": [
    "current node",
    "> 1%"
  ],
  "dependencies": {
    "@types/node": "^14.14.22"
  }
}
